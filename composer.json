{
	"name": "automattic/jetpack-sync",
	"description": "Everything needed to allow syncing to the WP.com infrastructure.",
	"type": "library",
	"license": "GPL-2.0-or-later",
	"require": {
		"automattic/jetpack-connection": "1.23.0",
		"automattic/jetpack-constants": "1.6.1",
		"automattic/jetpack-options": "1.11.0",
		"automattic/jetpack-roles": "1.4.1",
		"automattic/jetpack-status": "1.7.1"
	},
	"autoload": {
		"classmap": [
			"src/"
		]
	},
	"repositories": [],
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"mirror-repo": "Automattic/jetpack-sync"
	}
}
